{
  "main": {
    "autolock": true,
    "ignore_clsw": true,
    "timeout_seq": "no",
    "rotate_direction": "left",
    "authorize_external_users": true,
    "authorize_internal_users": false
  },
  "mqtt": {
    "broker_address": "localhost",
    "mqtt_port": 1883,
    "publish": {
      "boot": {
        "topic": "/autolock/boot",
        "message": "start"
      }
    },
    "subscribe": {
      "open": {
        "topic": "/door/open",
        "message": "1"
      },
      "close": {
        "topic": "/door/close",
        "message": "1"
      },
      "relay": {
        "topic": "/door/relay",
        "message": "1"
      },
      "change_config": {
        "topic": "/autolock/change_conf"
      }
    }
  },
  "line": {
    "channel_token": "token",
    "user_ids": ["id1", "id2"]
  },
  "slack": {
    "channel_token": "token",
    "send_channel": "#general"
  }
}
